#include <bits/stdc++.h>
#define ll long long int
using namespace std;
int main() {
    int t;
    cin>>t;
    while(t--){
        int x,y;
        cin>>x>>y;
        int arr[x+5];
        int res=0;
        for(int i=0;i<x;i++){
            cin>>arr[i];
            res=res+arr[i];
        }
        if(res==y){
            cout<<"YES"<<endl;
        }
        else{
            cout<<"NO"<<endl;
        }
    }
}
