#include <bits/stdc++.h>
#define ll long long int

using namespace std;
int main() {
   int t;
   cin>>t;
   while(t--){
       int arr[6];
       for(int i=0;i<4;i++){
           cin>>arr[i];

       }
       sort(arr,arr+4);
       ll x=min(arr[0],arr[1]);
       ll y=min(arr[2],arr[3]);
       cout<<x*y<<endl;

   }

}
